{
  "name": "@nestia/migrate",
  "version": "0.19.0",
  "description": "Migration program from swagger to NestJS",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "bin": {
    "@nestia/migrate": "lib/executable/migrate.js"
  },
  "scripts": {
    "build": "rimraf lib && tsc && rollup -c",
    "bundle": "ts-node src/executable/bundle.ts",
    "dev": "rimraf lib && tsc --watch",
    "package:next": "npm publish --access public --tag next",
    "prepare": "ts-patch install && typia patch && npm run bundle",
    "test": "node lib/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "keywords": [
    "migration",
    "swagger",
    "openapi generator",
    "NestJS",
    "nestia",
    "SDK",
    "RPC",
    "Mockup Simulator"
  ],
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestia/core": "^3.18.0",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.18.0",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-fastify": "^10.3.8",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cli-progress": "^3.11.5",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.3.3",
    "@types/swagger-ui-express": "^4.1.6",
    "chalk": "4.1.2",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.19.2",
    "rimraf": "^5.0.1",
    "rollup": "^4.24.3",
    "serialize-error": "^4.1.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "tgrid": "^1.0.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.2.1",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@samchon/openapi": "^2.0.0-dev.20241127-2",
    "commander": "10.0.0",
    "inquirer": "8.2.5",
    "prettier": "^3.2.5",
    "tstl": "^3.0.0",
    "typescript": "^5.5.4",
    "typia": "^7.0.0-dev.20241027-2"
  },
  "files": [
    "lib",
    "src",
    "!lib/test",
    "!src/test",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
